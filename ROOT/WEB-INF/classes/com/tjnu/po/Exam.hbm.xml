<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping 
			PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.tjnu.po.Exam" table="exam_tb"><!-- 每个class对应一个持久化对象 -->
		<id name="StudentId" type="string"><!-- id元素用来定义主键标识,并指定主键生成策略 -->
			<generator class="assigned"/>
		</id>
		<property name="studentName"/><!-- 映射studentName属性 -->
		<property name="result"/><!-- 映射result属性 -->
		<property name="time"/><!-- 映射time属性 -->
	</class>
</hibernate-mapping>