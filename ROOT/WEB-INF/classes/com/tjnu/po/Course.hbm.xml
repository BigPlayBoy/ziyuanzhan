<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping 
			PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.tjnu.po.Course" table="course_tb"><!-- 每个class对应一个持久化对象 -->
		<id name="courseId" type="int"><!-- id元素用来定义主键标识,并指定主键生成策略 -->
			<generator class="increment"/>
		</id>
		<property name="courseName"/><!-- 映射studentName属性 -->
		<property name="courseUrl"/><!-- 映射result属性 -->
		<property name="courseBeizhu"/><!-- 映射time属性 -->
	</class>
</hibernate-mapping>