<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping 
			PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.tjnu.po.VideoSource" table="videoSource_tb"><!-- 每个class对应一个持久化对象 -->
		<id name="videoSourceId" type="int"><!-- id元素用来定义主键标识,并指定主键生成策略 -->
			<generator class="increment"/>
		</id>
		<property name="videoId"/><!-- 映射videoId属性 -->
		<property name="videoName"/><!-- 映射videoName属性 -->
		<property name="videoUrl"/><!-- 映射videoUrl属性 -->
		<property name="videoDetail"/><!-- 映射videoDetail属性 -->
		<property name="videoImgUrl"/><!-- 映射videoImgUrl属性 -->
	</class>	
</hibernate-mapping>